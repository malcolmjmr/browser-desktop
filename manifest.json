{
    "manifest_version": 3,
    "name": "Browser Desktop",
    "version": "1.0",
    "description": "Manage browser windows and tabs with a visual desktop interface",
    "permissions": [
      "tabs",
      "tabGroups",
      "storage",
      "favicon"
    ],
    "action": {
      "default_title": "Clear Tabs"
    },
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }